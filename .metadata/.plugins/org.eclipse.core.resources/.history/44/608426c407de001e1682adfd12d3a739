dtmc	
module distance1
	
	s : [0..8] init 0;
	
	
	[]	s=0 -> (0.8):(s'=2)+(0.2):(s'=1);	
	[]	s=1 -> 1.0:(s'=8);	
	
	[]	s=2 -> 1.0:(s'=3);	
	
	[]	s=3 -> 1.0:(s'=4);	
	
	
	[]	s=4 -> (0.4):(s'=5)+(1.0):(s'=7);	
	[]	s=5 -> 1.0:(s'=6);	
	
	[]	s=6 -> 1.0:(s'=4);	
	
	[]	s=7 -> 1.0:(s'=8);	
	
	
	
endmodule


rewards"cost"
	s=1 : 7.0;	
endrewards

rewards"time"
	s=5 : 2.5;	
endrewards

